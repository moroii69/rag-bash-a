# ==========================================
# Environment Variables Example (.env.example)
# ==========================================
# Copy this file to .env.local and fill in
# your credentials before running the project.
# Do NOT commit real secrets to version control.
# ==========================================


# -----------------------------
# Vercel Authentication
# -----------------------------
# Used for deployment & CI/CD authentication
VERCEL_OIDC_TOKEN=your_vercel_oidc_token_here


# -----------------------------
# Google Generative AI
# -----------------------------
# API key for accessing Google Generative AI models
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here


# -----------------------------
# Clerk Authentication
# -----------------------------
# Public key for client-side
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
# Secret key for server-side
CLERK_SECRET_KEY=your_clerk_secret_key_here


# -----------------------------
# Database Configuration
# -----------------------------
# PostgreSQL/Neon database connection string
NEON_DATABASE_URL=postgresql://<username>:<password>@<host>:<port>/<database>?sslmode=require&channel_binding=require